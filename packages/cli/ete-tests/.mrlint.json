{
  "type": "library",
  "private": true,
  "testRunner": "jest",
  "rules": {
    "ts-config": {
      "exclude": [
        "./src/**/__test__/**/generated"
      ]
    },
    "jest": {
      "testMatch": [
        "**/*.test.ts"
      ]
    },
    "depcheck": {
      "ignores": [
        "form-data",
        "jest-environment-jsdom"
      ]
    }
  }
}