{
  "type": "cli",
  "cliName": "seed",
  "additionalFiles": ["fern/"],
  "environment": {
    "environments": {
      "prod": {
        "cliName": "seed",
        "cliPackageName": "@fern-api/seed-cli"
      }
    },
    "variables": []
  },
  "rules": {
    "package-json": {
      "scripts": {
        "dist:cli": "node build.cjs && cp -R ../../test-definitions/fern dist",
        "publish:cli": "turbo dist:cli && cd dist && yarn npm publish --access public",
        "generate": "fern generate --local"
      }
    }
  },
  "private": true
}
