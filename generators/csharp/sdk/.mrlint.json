{
  "type": "cli",
  "cliName": "fern-csharp-sdk",
  "environment": {
    "environments": {
      "prod": {
        "cliName": "fern-csharp-sdk",
        "cliPackageName": "@fern-api/fern-csharp-sdk"
      }
    }
  },
  "rules": {
    "depcheck": {
      "ignores": ["@fern-fern/ir-sdk"]
    },
    "duplicate-dependencies": {
      "exclude": ["@fern-fern/ir-sdk"]
    },
    "package-json": {
      "scripts": {
        "dockerTagLatest": "turbo dist:cli && docker build -f ./Dockerfile -t fernapi/fern-csharp-sdk:latest ../../.."
      }
    }
  },
  "private": false
}
