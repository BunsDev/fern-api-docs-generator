{
  "type": "cli",
  "cliName": "fern-ruby-sdk",
  "environment": {
    "environments": {
      "prod": {
        "cliName": "fern-ruby-sdk",
        "cliPackageName": "@fern-api/fern-ruby-sdk"
      }
    }
  },
  "rules": {
    "depcheck": {
      "ignores": [
        "ts-loader",
        "@fern-fern/ir-sdk"
      ],
      "ignore-patterns": [
        "docker/bundle.js"
      ]
    },
    "duplicate-dependencies": {
      "exclude": [
        "@fern-fern/ir-sdk"
      ]
    },
    "package-json": {
      "scripts": {
        "dockerTagLatest": "pnpm dist:cli && docker build -f ./Dockerfile -t fernapi/fern-ruby-sdk:latest ../../..",
        "dockerTagVersion": "pnpm dist:cli && docker build -f ./Dockerfile -t fernapi/fern-ruby-sdk:${0} ../../.."
      }
    }
  },
  "private": false
}