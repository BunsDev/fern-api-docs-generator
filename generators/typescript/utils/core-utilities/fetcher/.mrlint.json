{
  "type": "library",
  "commonJs": true,
  "private": false,
  "testRunner": "jest",
  "rules": {
    "duplicate-dependencies": {
      "exclude": [
        "@fern-fern/ir-sdk"
      ]
    },
    "depcheck": {
      "ignores": [
        "@types/jest",
        "@types/node",
        "node-fetch",
        "qs",
        "@types/node-fetch",
        "@types/qs",
        "jest-environment-jsdom"
      ]
    }
  }
}