{
  "type": "cli",
  "cliName": "fern-swift-model",
  "environment": {
    "environments": {
      "prod": {
        "cliName": "fern-swift-model",
        "cliPackageName": "@fern-api/fern-swift-model"
      }
    }
  },
  "rules": {
    "depcheck": {
      "ignores": [
        "@fern-fern/ir-sdk"
      ]
    },
    "duplicate-dependencies": {
      "exclude": [
        "@fern-fern/ir-sdk"
      ]
    },
    "package-json": {
      "scripts": {
        "dockerTagLatest": "yarn dist:cli && docker build -f ./Dockerfile -t fernapi/fern-swift-model:latest ../../..",
        "dist:cli": "yarn compile && node build.cjs"
      }
    }
  },
  "private": false
}